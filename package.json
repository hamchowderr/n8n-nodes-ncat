{
	"name": "n8n-nodes-ncat",
	"version": "0.1.2",
	"description": "n8n node for the No Code Architects Toolkit - Self-hosted media processing API for video, audio, and image manipulation",
	"license": "MIT",
	"homepage": "https://github.com/hamchowderr/n8n-nodes-ncat",
	"keywords": [
		"n8n-community-node-package",
		"n8n",
		"nca-toolkit",
		"media-processing",
		"video-processing",
		"audio-processing"
	],
	"author": {
		"name": "Tylan Miller",
		"email": "admin@otakusolutions.io"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/hamchowderr/n8n-nodes-ncat.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/NcaToolkitApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/NcaToolkit/NcaToolkit.node.js"
		]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	},
	"release-it": {
		"git": {
			"requireBranch": "master"
		}
	}
}
